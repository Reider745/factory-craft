{
	"$schema": "https://raw.githubusercontent.com/zheka2304/innercore-mod-toolchain/develop/toolchain/schemas/make.schema.json",
	"info": {
		"name": "FactoryCraft",
		"author": "Reider745",
		"version": "1.0",
		"description": "description",
		"clientOnly": false
	},
	"development": {
		"comparingMode": "size"
	},
	"resources": [
		{
			"path": "assets/res",
			"type": "resource_directory"
		},
		{
			"path": "assets/gui",
			"type": "gui"
		}
	],
	"sources": [
		{
			"source": "launcher.js",
			"type": "launcher",
			"language": "javascript"
		}
	],
	"compile": [
		{
			"source": "java",
			"type": "java"
		}
	],
	"additional": [
		{
			"source": "icstd.json",
			"targetDir": "."
		},
		{
			"source": "assets/lang",
			"targetDir": "."
		},
		{
			"source": "assets/windows",
			"targetDir": "."
		}
	]
}
